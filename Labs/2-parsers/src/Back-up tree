EXPR : //	OPERATOR {   $$ = new AddOperator( $1, $3 ); }
	| FUNCTION
	| ASSIGNMENT {   $$ = new AssignOperator( $1, $3 ); }
	| LOGIC
	| TERM { $$ = $1; }

FUNCTION : CONTAINER T_LBRACKET CONTAINER* T_RBRACKET T_LCURLBRACKET EXPR T_RCURLBRACKET


/*LOGIC : EXPR T_GT EXPR
	| EXPR T_GTOE EXPR
	| EXPR T_LT EXPR
	| EXPR T_LTOE EXPR 
	| EXPR T_NOT EXPR 
	| EXPR T_NOTEQUALS EXPR
	| EXPR T_EQUALS EXPR
	| EXPR T_AND EXPR
	| EXPR T_OR EXPR
	| EXPR T_XOR EXPR


/* OPERATOR : EXPR T_MUL EXPR {   $$ = new MulOperator( $1, $3 ); }
	| EXPR T_DIV EXPR {   $$ = new DivOperator( $1, $3 ); }
	| EXPR T_ADD EXPR {   $$ = new AddOperator( $1, $3 ); }
	| EXPR T_SUB EXPR {   $$ = new SubOperator( $1, $3 ); }
	| EXPR T_REMAINDER EXPR {   $$ = new RemainOperator( $1, $3 ); }
	| EXPR T_INCREMENT {   $$ = new IncrementOperator( $1 ); }
	| EXPR T_DECREMENT {   $$ = new DecrementOperator( $1 ); }
	| EXPR T_LAND EXPR {   $$ = new LogicalAndOperator( $1, $3 ); } */

ASSIGNMENT : CONTAINER T_ASSIGN EXPR
//	| CONTAINER T_ADDASSIGN EXPR
//	| CONTAINER T_SUBASSIGN EXPR

CONTAINER : T_INT T_VARIABLE 
/*	| T_DOUBLE T_VARIABLE
	| T_SHORT T_VARIABLE
	| T_LONG T_VARIABLE
	| T_CHAR T_VARIABLE
	| T_FLOAT T_VARIABLE
	| T_SIGNED CONTAINER
	| T_UNSIGNED CONTAINER
	| T_VOID CONTAINER
	| T_VARIABLE */

TERM :  TERM T_TIMES FACTOR 
	| TERM T_DIVIDE FACTOR 
	| FACTOR               { $$ = $1; }



FACTOR : T_NUMBER	{   $$ = new Number( $1 ); }
	| T_VARIABLE   
	| T_LBRACKET EXPR T_RBRACKET { $$ = $2; } 

}
